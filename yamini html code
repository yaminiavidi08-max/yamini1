<body>
<div class="container">

    <h1>Credit Card Fraud Detection</h1>
    <img src="data:image/jpeg;base64," alt="Credit Card">
    <form onsubmit="checkFraud(event)">
        <input type="text" id="card_number" placeholder="Enter 12-digit Card Number" required><br>
        <input type="number" id="amount" placeholder="Transaction Amount (â‚¹)" required><br>
        <input type="text" id="location" placeholder="Transaction Location" required><br>
        <button type="submit">Check Fraud</button>
    </form>
    <div class="result" id="result"></div>
</div>
<script>
function checkFraud(event) {
    event.preventDefault();
    let cardNumber = document.getElementById("card_number").value.trim();
    let amount = document.getElementById("amount").value;
    let location = document.getElementById("location").value.toLowerCase();
    let resultDiv = document.getElementById("result");
    if (!/^\d{12}$/.test(cardNumber)) {
        resultDiv.style.color = "red";
        resultDiv.innerHTML = " Please enter a valid 12-digit credit card number!";
        return;
    }
    if (amount > 5000 || (location !== "india" && location !== "us" && location !== "uk")) {
        resultDiv.style.color = "red";
        resultDiv.innerHTML = " This transaction might be FRAUDULENT!<br>Please contact your bank.";
    } else {
        resultDiv.style.color = "green";
        resultDiv.innerHTML = "Transaction looks SAFE.<br>No action required.";
    }
}
</script>
